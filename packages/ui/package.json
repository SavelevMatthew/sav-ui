{
  "name": "@savelevmatthew/ui",
  "description": "Basic UI-kit for testing different integrations",
  "repository": "https://github.com/SavelevMatthew/sav-ui",
  "author": "Matthew Savelev <savelevmatthew@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:tokens": "rm -rf ./tokens/sets && node ./bin/transform-tokens.js",
    "build:styles": "style-dictionary build --config ./sd.config.js",
    "build:vars": "yarn build:tokens && yarn build:styles",
    "build:src": "webpack",
    "build:types": "tsc --emitDeclarationOnly --declarationMap",
    "build": "yarn build:vars && yarn build:src && yarn build:types",
    "dev": "yarn build:vars && start-storybook -p 6006",
    "build:sb": "build-storybook"
  },
  "exports": {
    "./colors": "./dist/colors/index.js"
  },
  "dependencies": {
    "antd": "^4.22.8",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "16.9.17",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-loader": "^4.0.0",
    "style-dictionary": "^3.7.1",
    "token-transformer": "^0.0.25",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
